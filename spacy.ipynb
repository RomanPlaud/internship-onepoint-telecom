    "## import wordnet \n",
    "import nltk\n",
    "from nltk.corpus import wordnet as wn\n",
    "\n",
    "#download wordnet\n",
    "nltk.download('wordnet')\n",
    "\n",
    "#find synonyms of a word\n",
    "synonyms = []\n",
    "for syn in wn.synsets(\"party\"):\n",
    "    for l in syn.lemmas():\n",
    "        synonyms.append(l.name())\n",
    "print(set(synonyms))\n"
